{
  "name": "GitHub Command Bar (unofficial)",
  "version": "0.1",
  "description" : "To use, type 'gh' plus the github command into the Omnibox.", 
  "homepage_url": "https://github.com/eduardocampano/github-launch-chrome",
  "background": {
	"page" : "background.html"
  },
  "omnibox": { "keyword" : "gh" },
  "manifest_version": 2,
  "icons": { 
        "16": "icon-small.png",
        "48": "icon-medium.png",
        "128": "icon-big.png"
  },
  "permissions": [
      "tabs",
	  "https://github.com/"
  ]
}
